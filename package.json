{
  "name": "node-server",
  "version": "0.1.0",
  "private": true,
  "publishConfig": {
    "registry": "https://nexus.area51.onl/repository/npm-private/"
  },
  "scripts": {
    "build": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src",
